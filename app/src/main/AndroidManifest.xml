<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raymond.randomexercise">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".activities.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.AnimActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".example.photo.ActivityAnimations"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name=".example.photo.PictureDetailsActivity"
            android:label="@string/subactivity_name"
            android:theme="@style/Transparent" >
        </activity>

        <activity android:name=".example.particle.MainActivity"/>
        <activity android:name=".activities.PostListActivity"/>
    </application>

</manifest>